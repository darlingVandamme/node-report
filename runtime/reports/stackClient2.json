{
  "name": "stack Monthly",
  "description": "Test report for Mysql stackexchange Client side functionality test ",
  "version": "0.0.1",

  "require": ["query","server"],

  "datasets":[
    {
      "name" : "query",
      "show": "*",
      "display" : {
        "template": "form",
        "position": "nav"
      },
      "columns": {
        "startDate": {
          "template" : "date",
          "required" : true
        },
        "endDate": {
          "template" : "date",
          "required" : true
        }
      }
    },
    {
      "name": "main",
      "description": "",
      "channel": "StackExchange",
      "display" : {
         "template": "table",
         "class": "zebra"
      },
      "query": { "file": "stackDaily.sql"},
      "show": "*",
      "columns": {
      }
    },
    {
      "name" : "months",
      "description" : "",
      "display" : {
        "template": "table",
        "position": "main",
        "class": "zebra"
      },
      "channel" : "StackExchange",
      "show" : "*",
      "query": "select year(CreaionDate) year, month(CreaionDate) month , count(*) as posts, cast(sum(viewCount) as integer) as views from posts\nwhere CreaionDate between {{query.startDate}} and {{query.endDate}} group by year, month",
      "columns": {

      }
    },

    {
      "name" : "click",
      "description" : "",
      "display" : {
        "template": "f_clickTest",
        "position" : "main"
      },
      "source" : "months",
      "x": "d",
      "y": "posts"

    }


  ],

  "output": {
    "json": {
      "include": ["query","paging","main","log"],
      "type" : "json"
    },
    "html": {
      "include": ["query","block","months","click","log","main"],
      "template": "general",
      "type" : "hbs",
      "classes": {
        "main": "row",
        "nav": "hide_anim"
      }
    },
    "arg": {
      "include" : ["args","query"],
      "extension" : "html",
      "type": "hbs",
      "template": "general"

    }
  }
}