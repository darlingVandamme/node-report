{
  "name": "server",
  "description": "Server info ",
  "keywords": ["demo","server"],

  "require": ["query","server","headers"],
  "datasets": [
    {
      "name" : "query",
      "show": "*",
      "display" : {
        "template": "row",
        "position": "main"
      }
    },

    {
      "name": "server",
      "show": "*",
      "display" :{
        "template": "table",
        "position": "main"
      }

    },
    {
      "name": "headers",
      "show": "*",
      "display" :{
        "template": "row",
        "position": "main"
      }

    }
  ],
  "output": {
    "json": {
      "include": [
        "query",
        "server",
        "headers"
      ],
      "type": "json"
    },
    "html": {
      "include": [
        "server",
        "query",
        "headers"
      ],
      "classes": {
        "header": "hidden",
        "left_column": "hidden",
        "nav": "",
        "main": ""
      },
      "template": "general",
      "type": "hbs",
      "main" : "row"
    },
    "arg": {
      "include": [
        "query"
      ],
      "extension": "html",
      "type": "hbs",
      "template": "general"
    }
  }

}