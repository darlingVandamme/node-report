{
  "name": "stack List",
  "description": "Test report for Mysql stackexchange",

  "require": ["query","session"],

  "datasets":[
    {
      "name" : "args",
      "show": "*",
      "display": {
        "template" :  "table",
        "position" : "hidden"
      },
      "channel" : "copy",
      "from": "query",

      "columns": {
        "tagContains": {
          "type" : "format",
          "default": "",
          "format" : "%<{{tag}}>%"
        }
      }
    },

    {
      "name" : "main",
      "description" : "",
      "display":{
        "template" : "table",
        "class" : "collapseable",
        "position": "main",
        "styleColumn" : "class",
        "classColumn" : "class"
      },
      "position": "main",
      "channel" : "StackExchange",

      "query" : "select * from posts where Tags like {{args.tagContains}} limit {{paging.limit}} offset {{paging.offset}}",
      "query1" : "select * from posts where Tags like '%sample%' limit 10",
      "show" : "*",
      "show2" : "Score,Body,Score,Id",
      "columns": {
        "class": {
          "type" : "hbs",
          "format" : "{{#if AcceptedAnswerId}}accepted{{/if}}"
        },

        "accept": {
          "type" : "hbs",
          "template" : "td",
          "format" : "{{#if AcceptedAnswerId}}accepted{{/if}}"
        },
        "Body": {
          "type" : "default",
          "order": 145,
          "class": "hidden"
        }
      }
      }
  ],

  "output": {
    "json": {
      "include": ["query","paging","main","log"],
      "type" : "json"
    },
    "html": {
      "include": ["query","args","paging","server","main","log"],
      "argForm": "query",
      "template": "general",
      "type" : "hbs"
    },
    "arg": {
      "include" : "main",
      "type": "hbs",
      "page": "argtest"
    },
    "ds": {
      "include" : ["main"],
      "template": "div",
      "type" : "hbs",
      "page": "argtest"
    }

  }
}