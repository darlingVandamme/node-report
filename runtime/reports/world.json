{
  "name": "stack User",
  "description": "Test report for Mysql stackexchange",

  "require": ["query","server"],

  "datasets":[
  {
    "name" : "query",
    "show": "*",
    "type" :  "form",
    "position" : "nav",
    "columns": {
      "country" : {
        
            },
      "countrycontains": {
        "template" : "hidden",
        "type": "format",
        "format": "%{{country}}%"
      }
    }
  },

  {
    "name" : "Cards",
    "description" : "",
    "type" : "cards_world",
    "source" : "world",
    "class" : "zebra",
    "queryFile" : "country.sql",
    "show" : "*",
    "columns" : {
      "url" : {
        "type"  :"format",
        "format" : "http://localhost:3000/report/worldCities.html?code={{Code}}"
      }

    }
    }

  ],

  "output": {
    "json": {
      "include": ["query","Cards","paging","main","log"],
      "type" : "json"
    },
    "html": {
      "include": ["query","Cards","args","server","log"],
      "argForm": {
          "dataset": "args",
          "extension" : "html"
      },
      "template": "general",
      "type" : "hbs"
    },
    "arg": {
      "include" : ["args","query"],
      "target" : "html",
      "argForm": {
        "dataset": "args",
        "extention" : "html"
      },
      "type": "hbs",
      "template": "general"

    }
  }
}