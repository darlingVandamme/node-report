<header>
    {{> logo}}
    <h1 class="report_name">{{name}}</h1>
    <div>{{momentString}} Prof: {{profile}}ms  </div>

    {{#each datasets}}
        {{#if this.position.header}}
            {{> serverDS}}
        {{/if}}
    {{/each}}
</header>

<div class="float_right">
    <div class="hide_button_right toggle_hide icon toggled" data-target=".checkboxes" data-level="0" data-toggleclass="hide_anim">
    </div>

    <div class="checkboxes hide_anim">
        {{#each datasets}}
            {{> checkbox}}
        {{/each}}
    </div>
</div>


<div class="container">
    <div class="left_column">
        <div class="hide_button toggle_hide icon" data-target="nav" data-level="1" data-toggleclass="hide_anim">
        </div>
        <nav>
            {{#each datasets}}
                {{#if this.position.nav}}
                    {{> serverDSnav}}
                {{/if}}
            {{/each}}
        </nav>
    </div>

    <div id="overlay" class="hidden">
        <div class="xmark toggle_hide" data-target="#overlay" data-level="0" data-toggleclass="hidden">
        </div>
        <div class="table_info">
        </div>
    </div>

    <main>
        {{#each datasets}}
            {{#if this.position.main}}
                {{> serverDS}}
            {{/if}}
        {{/each}}
        <div class="tabs">
            {{#each datasets}}
                {{#if this.position.tabs}}
                    {{> tabs}}
                {{/if}}
            {{/each}}
        </div>
        <div class="tab_tables">
            {{#each datasets}}
                {{#if this.position.tabs}}
                    {{> serverDStabs}}
                {{/if}}
            {{/each}}
        </div>
    </main>
</div>

<div class="hidden" >
    {{#each datasets}}
        {{#if this.position.hidden}}
            {{> serverDS}}
        {{/if}}
    {{/each}}
</div>

<script language="JavaScript">
    const report = {
        name: "{{name}}",
        moment: "{{moment}}",
        momentString: "{{momentString}}",
        profile: "{{profile}}",
        url: "{{{url}}}",
        style: "{{style}}",
        datasets:[
            {{#each datasets}}
                {
                    name:"{{this.name}}",
                    columns: {},
                    data: [ {{{this.JSONData}}}]
                },
            {{/each}}
                ]
    }
    </script>
