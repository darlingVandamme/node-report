{{#with dataset}}
    <svg width="200" height="200" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
        <style>
            .info{
                opacity:0%;
            }
            .info:hover{
                opacity:100%;
            }
            .zone{
                fill: transparent;
                stroke: transparent;
            }
        </style>
        <g fill="white" stroke="green" stroke-width="2">
            <line x1="0" y1="100" x2="0" y2="0" />
            <line x1="0" y1="100" x2="100" y2="100" />
        </g>
        {{#each options.graph}}
            <path
                {{#with (lookup ../columns this)}}
                fill="transparent" stroke="{{color}}"
                {{/with}}
                d="M0 100
    {{#each ../displayData}}
                    {{#graph this ../this}}
{{#if @first}}M{{/if}}{{#unless @first}}L{{/unless}}{{coord.x1}} {{coord.y2}}
                    {{/graph}}
    {{/each}} "/>
    {{#each ../displayData}}
        {{#graph this ../this}}
            <g class="info">
                <rect class="zone" x="{{coord.x1}}" y="0" width="10" height="100"></rect>
                <rect fill="lightgray" x="{{coord.x1}}" y="{{coord.y2}}" width="10" height="10"></rect>
                <text x="{{coord.x1}}" y="{{coord.y2}}" dx="3" dy="3">{{this.data.posts.value}}</text>
            </g>
        {{/graph}}
    {{/each}} "/>

        {{/each}}
    </svg>
{{/with}}