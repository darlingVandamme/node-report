{{#with report}}
    {{#each data}}
        <div id="dataset_{{this.name}}" class="{{this.options.style}}  data-src="{{this.url}}">
            {{{this.html}}}
        </div>
    {{/each}}
<script language="JavaScript">
    let datasets = {
        {{#each data}}
            "{{this.name}}": {{{jsData this.name}}} {{#unless @last}}, {{/unless}}
        {{/each}}
    }
</script>
{{/with}}