{{#with report}}

<div class="dropdown">
    <div class="dropdown-content">
        {{position "dropdown"}}
    </div>
</div>

<div class="float_right">
    <div class="hide_button_right toggle_hide icon toggled" data-target=".checkboxes" data-level="0" data-toggleclass="hide_anim">
    </div>
    <div class="checkboxes hide_anim">
        {{#each datasets}}
            {{> checkbox}}
        {{/each}}
    </div>
</div>

<div class="main_container">
    <div class="left_column {{@root.output.classes.left_column}}" >
        <div class="hide_button toggle_hide ball" data-target="nav" data-level="1" data-toggleclass="hide_anim">
        </div>
        <nav class="{{@root.output.classes.nav}}">
            {{position "nav" }}
            {{paging}}
        </nav>
    </div>
    <main>
        <div class="main-content {{@root.output.classes.main}}">
            {{position "main" }}
        </div>
    </main>
</div>

<div class="hidden" >
    {{position "hidden"}}
</div>

<script language="JavaScript">
    const report = {
        name: "{{name}}",
        moment: "{{moment}}",
        profile: "{{profile}}",
        url: "{{{url}}}",
        style: "{{style}}",
        data:{
            {{#each data}}
            "{{this.name}}": {{{jsData this.name html=false}}} {{#unless @last}}, {{/unless}}
            {{/each}}
            }
    }

</script>
<script language="JavaScript" type="module">
        {{position "function"}}
</script>
{{/with}}