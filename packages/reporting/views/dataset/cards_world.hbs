{{#with dataset}}
<div class='dataset_cards'>
{{#each displayData}}
    <div class="card">
        <a href="{{this.data.url.value}}">
            <div class="card_name">
                <h2>{{this.data.Name.display}}</h2>
            </div>
            <div class="card_id"> <h2>Population: {{this.data.Population.display}}</h2></div>
            
            <div class="card_aboutme">
                Cities: 
                <div class="about_me_text">
                    {{this.data.Cities.display}}
                </div>
            </div>
        </a>
    </div>
{{/each}}
</div>
{{/with}}