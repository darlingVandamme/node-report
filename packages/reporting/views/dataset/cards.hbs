{{#with dataset}}
<div {{datasetID this.name}}>
<div class='dataset_cards'>
{{#each displayData}}
    <div class="card">
        <a href="{{this.data.url.value}}">
            {{#if this.data.image}}
                <div class="card_image_container">
                    <img  class="card_image" src="{{this.data.image.value}}" alt="profile">
                </div>
            {{/if}}
            
            <div class="card_name">
                <h2>{{this.data.name.display}}</h2>
            </div>
            <div class="card_id"> <h3>id: {{this.data.Id.display}}</h3></div>
            
            <div class="card_aboutme">
                About me: 
                <div class="about_me_text">{{this.data.AboutMe.display}}</div>
            </div>
        </a>
    </div>
{{/each}}
</div>
</div>
{{/with}}

