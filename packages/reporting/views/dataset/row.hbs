{{#with dataset}}
<div {{datasetID this.name}}>
    <h2 >{{this.options.header}}</h2>
    <div >{{this.description}}</div>
    {{#each displayData}}
    <div class="dataset_row" data-rowNr="{{this.rowNr}}">
        {{#each this.data}}
            <div class="{{this.class}}"><span class="header">{{this.column}}</span> {{this.value}}</div>
        {{/each}}
    </div>
{{/each}}
</div>
{{/with}}