{{#with dataset}}
    <div {{datasetID this.name}}>
        <h2>{{this.options.header}}</h2>
        <form  method="get" {{link this "action"}} ">
            <div class="inputs">
                {{#each displayData}}
                    {{#each this.data}}
                            {{formElement this @root.dataset}}
                    {{/each}}
                {{/each}}
            </div>
            <input type='submit'>
        </form>
    </div>
{{/with}}